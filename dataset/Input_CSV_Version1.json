{
	"name": "Input_CSV_Version1",
	"properties": {
		"linkedServiceName": {
			"referenceName": "BlobStorage_LinkedService",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"year": {
				"type": "string"
			},
			"month": {
				"type": "string"
			},
			"day": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": ".*csv",
				"folderPath": {
					"value": "landing/Full_load/@{dataset().year}/@{dataset().month}/@{dataset().day}\n",
					"type": "Expression"
				},
				"container": "input"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "CustomerID",
				"type": "String"
			},
			{
				"name": "CustomerName",
				"type": "String"
			},
			{
				"name": "ContactNumber",
				"type": "String"
			},
			{
				"name": "City",
				"type": "String"
			}
		]
	}
}